# Dockerfile
FROM python:3.8

# Install any necessary libraries
RUN pip install scikit-learn joblib

# Copy your training script and model
COPY ../evp_demo.py /
COPY probability_model.joblib /
COPY life_time_model.joblib /
CMD ["python", "evp_demo.py"]
